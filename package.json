  "author": "Expeditie Admins",
  "contributors": [
    "Maurice Meedendorp <mauricemeedendorp@gmail.com>",
    "Martijn Atema <martijn@atema.one>"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Expeditie-Grensland/Website.git"
  },
  "license": "GPL-3.0",
  "type": "module",
  "engines": {
    "npm": "-",
    "yarn": "-",
    "pnpm": ">=7"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.540.0",
    "@fastify/cookie": "^9.3.1",
    "@fastify/formbody": "^7.4.0",
    "@fastify/multipart": "^8.2.0",
    "@fastify/session": "^10.7.0",
    "@fastify/static": "^7.0.2",
    "@fastify/view": "^9.0.0",
    "@inquirer/input": "^2.1.1",
    "@inquirer/select": "^2.2.1",
    "connect-redis": "^7.1.1",
    "dotenv": "^16.4.5",
    "fast-xml-parser": "^4.3.6",
    "fastify": "^4.26.2",
    "full-icu": "^1.5.0",
    "ldapauth-fork": "^5.0.5",
    "luxon": "^3.4.4",
    "marked": "^12.0.1",
    "migrate-mongo": "^11.0.0",
    "mime": "^4.0.1",
    "mongoose": "8.2.4",
    "pino": "^8.19.0",
    "pug": "^3.0.2",
    "qs": "^6.12.0",
    "redis": "^4.6.13",
    "zod": "^3.22.4",
    "zod-validation-error": "^3.0.3"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^6.0.0",
    "@mapbox/mapbox-gl-language": "^1.0.1",
    "@types/geojson": "^7946.0.14",
    "@types/jquery": "^3.5.29",
    "@types/luxon": "^3.4.2",
    "@types/mapbox-gl": "^3.1.0",
    "@types/mime": "^3.0.4",
    "@types/node": "^20.12.2",
    "@types/pug": "^2.0.10",
    "@types/qs": "^6.9.14",
    "@typescript-eslint/eslint-plugin": "^7.4.0",
    "@typescript-eslint/parser": "^7.4.0",
    "autoprefixer": "^10.4.19",
    "bootstrap": "^5.3.3",
    "concurrently": "^8.2.2",
    "core-js": "^3.36.1",
    "dotenv-cli": "^7.4.1",
    "esbuild": "^0.20.2",
    "esbuild-sass-plugin": "^3.2.0",
    "eslint": "^8.57.0",
    "eslint-nibble": "^8.1.0",
    "eslint-plugin-import": "^2.29.1",
    "geojson": "^0.5.0",
    "hls.js": "1.5.7",
    "jquery": "^3.7.1",
    "mapbox-gl": "^3.2.0",
    "nodemon": "^3.1.0",
    "pino-pretty": "^11.0.0",
    "postcss": "^8.4.38",
    "sass-embedded": "^1.72.0",
    "ts-unused-exports": "^10.0.1",
    "typescript": "^5.4.3"
  },
  "files": [
    "pnpm-lock.yaml",
    "dist",
    "migrate-mongo-config.js",
    "migrations"
  ],
  "bin": {
    "expeditiegrensland-website": "dist/server/bin/expeditiegrensland-website.js",
    "expeditiegrensland-cli": "dist/server/bin/expeditiegrensland-cli.js"
  },
  "scripts": {
    "start": "node dist/server/server.js",
    "cli": "dotenv -- node dist/server/cli/main.js",
    "build": "concurrently -c auto -m 1 'npm:build:*'",
    "build:clean": "node build/clean.js dist",
    "build:copy": "node build/copy.js dist",
    "build:errorpages": "node build/error-pages.js",
    "build:client": "node build/client.js dist",
    "build:styles": "node build/styles.js dist",
    "build:server": "tsc --project src/server/ --outDir dist/server/",
    "dev": "concurrently -c auto --restart-tries -1 -i --default-input-target run 'npm:dev:*(!clean)'",
    "dev:clean": "node build/clean.js dev",
    "dev:copy": "node build/copy.js dev",
    "dev:client": "node build/client.js dev",
    "dev:styles": "node build/styles.js dev",
    "dev:server": "tsc --project src/server/ --outDir dev/server/ --watch --preserveWatchOutput --sourceMap",
    "dev:run": "sleep 10 && dotenv -- nodemon --watch 'dev/server/**/*' --delay 5s --exec 'node --enable-source-maps --icu-data-dir=node_modules/full-icu' dev/server/server.js",
    "migrate-status": "dotenv -- migrate-mongo status",
    "migrate-create": "dotenv -- migrate-mongo create",
    "migrate-up": "dotenv -- migrate-mongo up",
    "migrate-down": "dotenv -- migrate-mongo down",
    "clean": "concurrently -c auto -m 1 'npm:*:clean'",
    "check": "concurrently -c auto -m 1 'npm:check:*(!nibble)'",
    "check:types:server": "tsc --noEmit --project src/server/tsconfig.json",
    "check:types:client": "tsc --noEmit --project src/client/tsconfig.json",
    "check:lint": "eslint . --ext ts,js,cjs,mjs",
    "check:nibble": "eslint-nibble . --ext ts,js,cjs,mjs",
    "check:unused:server": "ts-unused-exports src/server/tsconfig.json --maxIssues=9999999",
    "check:unused:client": "ts-unused-exports src/client/tsconfig.json --maxIssues=9999999"
  }
}
