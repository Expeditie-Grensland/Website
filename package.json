{
  "name": "@expeditie-grensland/expeditiegrensland",
  "version": "2.4.1",
  "description": "De Expeditie Grensland website & CLI",
  "author": "Expeditie Admins",
  "contributors": [
    "Maurice Meedendorp <mauricemeedendorp@gmail.com>",
    "Martijn Atema <martijn@atema.one>"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Expeditie-Grensland/Website.git"
  },
  "license": "GPL-3.0",
  "type": "module",
  "engines": {
    "npm": "-",
    "yarn": "-",
    "pnpm": ">=7"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.717.0",
    "@fastify/flash": "^6.0.2",
    "@fastify/formbody": "^8.0.1",
    "@fastify/multipart": "^9.0.1",
    "@fastify/secure-session": "^8.1.0",
    "@fastify/static": "^8.0.3",
    "@inquirer/input": "^4.1.1",
    "@inquirer/select": "^4.0.4",
    "dotenv": "^16.4.7",
    "fast-xml-parser": "^4.5.1",
    "fastify": "^5.2.0",
    "full-icu": "^1.5.0",
    "hash-wasm": "^4.12.0",
    "kysely": "^0.27.5",
    "luxon": "^3.5.0",
    "marked": "^15.0.4",
    "mime": "^4.0.6",
    "pg": "^8.13.1",
    "pino": "^9.6.0",
    "preact": "^10.25.3",
    "preact-render-to-string": "^6.5.12",
    "qs": "^6.13.1",
    "sodium-native": "^4.3.1",
    "zod": "^3.24.1",
    "zod-validation-error": "^3.4.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@types/eslint__js": "^8.42.3",
    "@types/geojson": "^7946.0.15",
    "@types/luxon": "^3.4.2",
    "@types/node": "^22.10.2",
    "@types/pg": "^8.11.10",
    "@types/qs": "^6.9.17",
    "@types/sodium-native": "^2.3.9",
    "concurrently": "^9.1.0",
    "esbuild": "^0.24.2",
    "eslint": "^9.17.0",
    "eslint-plugin-import-x": "^4.6.1",
    "geojson": "^0.5.0",
    "globals": "^15.14.0",
    "globby": "^14.0.2",
    "hls.js": "1.5.18",
    "kysely-codegen": "^0.17.0",
    "mapbox-gl": "^3.9.1",
    "nodemon": "^3.1.9",
    "pino-pretty": "^13.0.0",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.18.2"
  },
  "files": [
    "pnpm-lock.yaml",
    "dist"
  ],
  "bin": {
    "expeditiegrensland-website": "dist/server/server.js",
    "expeditiegrensland-cli": "dist/server/cli/main.js"
  },
  "scripts": {
    "start": "node dist/server/server.js",
    "start-cli": "node dist/server/cli/main.js",
    "build": "concurrently -c auto -m 1 --pad-prefix 'npm:build:*(!run)'",
    "build:clean": "node build/clean.js dist",
    "build:copy": "node build/copy.js dist",
    "build:errorpages": "tsx build/errorpages.ts",
    "build:client": "node build/client.js dist",
    "build:styles": "node build/styles.js dist",
    "build:server": "node build/server.js dist",
    "build:rev": "node build/rev.js",
    "build:compress": "node build/compress.js",
    "dev": "concurrently -c auto --restart-tries -1 -i --default-input-target run --pad-prefix 'npm:dev:*(!clean)'",
    "dev:clean": "node build/clean.js dev",
    "dev:copy": "node build/copy.js dev",
    "dev:client": "node build/client.js dev",
    "dev:styles": "node build/styles.js dev",
    "dev:server": "node build/server.js dev",
    "dev:run": "sleep 1 && nodemon --watch 'dev/server/**/*' --exec 'node --enable-source-maps --icu-data-dir=node_modules/full-icu' dev/server/server.js",
    "db:migrations": "tsx build/db-migrate.ts list",
    "db:create-migration": "tsx build/db-migrate.ts create",
    "db:migrate-only": "tsx build/db-migrate.ts latest",
    "db:migrate": "concurrently -c auto -m 1 --pad-prefix -n migrate,generate npm:db:migrate-only npm:db:generate",
    "db:migrate-only-up": "tsx build/db-migrate.ts up",
    "db:migrate-up": "concurrently -c auto -m 1 --pad-prefix -n migrate,generate npm:db:migrate-only-up npm:db:generate",
    "db:migrate-only-down": "tsx build/db-migrate.ts down",
    "db:migrate-down": "concurrently -c auto -m 1 --pad-prefix -n migrate,generate npm:db:migrate-only-down npm:db:generate",
    "db:generate": "kysely-codegen --url 'env(EG_DB_URL)' --out-file src/server/db/schema/types.ts",
    "clean": "concurrently -c auto -m 1 --pad-prefix 'npm:*:clean'",
    "check": "concurrently -c auto -m 1 --pad-prefix 'npm:check:*'",
    "check:types:server": "tsc --noEmit --project src/server/tsconfig.json",
    "check:types:client": "tsc --noEmit --project src/client/tsconfig.json",
    "check:lint": "eslint .",
    "stats": "node build/stats.js",
    "util:generate-secret": "node build/util-generate-secret.js"
  }
}
