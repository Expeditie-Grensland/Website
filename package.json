{
  "name": "@expeditie-grensland/expeditiegrensland",
  "version": "2.0.0",
  "description": "De Expeditie Grensland website & CLI",
  "author": "Expeditie Admins",
  "contributors": [
    "Maurice Meedendorp <mauricemeedendorp@gmail.com>",
    "Martijn Atema <martijn@atema.one>"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Expeditie-Grensland/Website.git"
  },
  "license": "GPL-3.0",
  "type": "module",
  "engines": {
    "npm": "-",
    "yarn": "-",
    "pnpm": ">=7"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.592.0",
    "@fastify/flash": "^5.2.0",
    "@fastify/formbody": "^7.4.0",
    "@fastify/multipart": "^8.3.0",
    "@fastify/secure-session": "^7.5.1",
    "@fastify/static": "^7.0.4",
    "@fastify/view": "^9.1.0",
    "@inquirer/input": "^2.1.9",
    "@inquirer/select": "^2.3.5",
    "dotenv": "^16.4.5",
    "fast-xml-parser": "^4.4.0",
    "fastify": "^4.27.0",
    "full-icu": "^1.5.0",
    "hash-wasm": "^4.11.0",
    "kysely": "^0.27.3",
    "ldapjs": "^3.0.7",
    "luxon": "^3.4.4",
    "marked": "^13.0.0",
    "mime": "^4.0.3",
    "pg": "^8.12.0",
    "pino": "^9.2.0",
    "pug": "^3.0.3",
    "qs": "^6.12.1",
    "sodium-native": "^4.1.1",
    "zod": "^3.23.8",
    "zod-validation-error": "^3.3.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.4.0",
    "@fullhuman/postcss-purgecss": "^6.0.0",
    "@mapbox/mapbox-gl-language": "^1.0.1",
    "@types/eslint__js": "^8.42.3",
    "@types/geojson": "^7946.0.14",
    "@types/jquery": "^3.5.30",
    "@types/ldapjs": "^3.0.6",
    "@types/luxon": "^3.4.2",
    "@types/mapbox-gl": "^3.1.0",
    "@types/node": "^20.14.2",
    "@types/pg": "^8.11.6",
    "@types/pug": "^2.0.10",
    "@types/qs": "^6.9.15",
    "@types/sodium-native": "^2.3.9",
    "autoprefixer": "^10.4.19",
    "bootstrap": "^5.3.3",
    "concurrently": "^8.2.2",
    "core-js": "^3.37.1",
    "dotenv-cli": "^7.4.2",
    "esbuild": "^0.21.5",
    "esbuild-sass-plugin": "^3.3.1",
    "eslint": "^8.57.0",
    "eslint-plugin-import-x": "^0.5.1",
    "geojson": "^0.5.0",
    "globals": "^15.4.0",
    "globby": "^14.0.1",
    "hls.js": "1.5.11",
    "jquery": "^3.7.1",
    "kysely-codegen": "^0.15.0",
    "mapbox-gl": "^3.4.0",
    "nodemon": "^3.1.3",
    "pino-pretty": "^11.2.1",
    "postcss": "^8.4.38",
    "sass-embedded": "^1.77.5",
    "ts-unused-exports": "^10.1.0",
    "tsx": "^4.15.2",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.13.0"
  },
  "files": [
    "pnpm-lock.yaml",
    "dist"
  ],
  "bin": {
    "expeditiegrensland-website": "dist/server/bin/expeditiegrensland-website.js",
    "expeditiegrensland-cli": "dist/server/bin/expeditiegrensland-cli.js"
  },
  "scripts": {
    "start": "dotenv -- node dist/server/server.js",
    "cli": "dotenv -- node dist/server/cli/main.js",
    "build": "concurrently -c auto -m 1 'npm:build:*(!run)'",
    "build:clean": "node build/clean.js dist",
    "build:copy": "node build/copy.js dist",
    "build:errorpages": "node build/errorpages.js",
    "build:client": "node build/client.js dist",
    "build:styles": "node build/styles.js dist",
    "build:server": "node build/server.js dist",
    "build:rev": "node build/rev.js",
    "build:compress": "node build/compress.js",
    "dev": "concurrently -c auto --restart-tries -1 -i --default-input-target run 'npm:dev:*(!clean)'",
    "dev:clean": "node build/clean.js dev",
    "dev:copy": "node build/copy.js dev",
    "dev:client": "node build/client.js dev",
    "dev:styles": "node build/styles.js dev",
    "dev:server": "node build/server.js dev",
    "dev:run": "sleep 1 && dotenv -- nodemon --watch 'dev/server/**/*' --exec 'node --enable-source-maps --icu-data-dir=node_modules/full-icu' dev/server/server.js",
    "db:migrations": "tsx build/db-migrate.ts list",
    "db:create-migration": "tsx build/db-migrate.ts create",
    "db:migrate-only": "tsx build/db-migrate.ts latest",
    "db:migrate": "concurrently -c auto -m 1 -n migrate,generate npm:db:migrate-only npm:db:generate",
    "db:migrate-only-up": "tsx build/db-migrate.ts up",
    "db:migrate-up": "concurrently -c auto -m 1 -n migrate,generate npm:db:migrate-only-up npm:db:generate",
    "db:migrate-only-down": "tsx build/db-migrate.ts down",
    "db:migrate-down": "concurrently -c auto -m 1 -n migrate,generate npm:db:migrate-only-down npm:db:generate",
    "db:generate": "kysely-codegen --url 'env(EG_DB_URL)' --out-file src/server/db/schema/types.ts",
    "clean": "concurrently -c auto -m 1 'npm:*:clean'",
    "check": "concurrently -c auto -m 1 'npm:check:*'",
    "check:types:server": "tsc --noEmit --project src/server/tsconfig.json",
    "check:types:client": "tsc --noEmit --project src/client/tsconfig.json",
    "check:lint": "eslint .",
    "check:unused:server": "ts-unused-exports src/server/tsconfig.json --allowUnusedTypes --ignoreFiles=src/server/db/schema/migrations/*",
    "check:unused:client": "ts-unused-exports src/client/tsconfig.json --allowUnusedTypes",
    "stats": "node build/stats.js",
    "util:generate-secret": "node build/util-generate-secret.js"
  }
}
