{
  "compileOnSave": false,
  "exclude": [
    "worker",
    "serviceWorker.ts"
  ],
  "compilerOptions": {
    "target": "es5",
    "module": "es6",
    "moduleResolution": "node",
    "outDir": "../../../dist/public/scripts",
    "lib": [
      "es5",
      "es2015.collection",
      "dom"
    ],
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true
  }
}
